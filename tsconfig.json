{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "library/*": ["src/library/*"],
      "containers/*": ["src/containers/*"],
      "res/*": ["src/library/res/*"],
      "src/*": ["src/*"],
    },
    "resolveJsonModule": true,
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */
  }
}
